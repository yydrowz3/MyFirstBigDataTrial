import $http from './index';

export const getUsers = () =>
  $http({
    url: '/admin/api/getUsers',
    method: 'get',
  });

export const getSchoolInfo = (data) =>
  $http({
    url: '/admin/api/getSchoolInfo',
    method: 'get',
    data,
  });

export const deleteOneSchoolInfo = (data) =>
  $http({
    url: '/admin/api/deleteOneSchoolInfo',
    method: 'post',
    data,
  });
